             xmlns:Local="clr-namespace:TestProjectXamarin.Views.Menu;assembly=TestProjectXamarin"
             xmlns:DetailView="clr-namespace:TestProjectXamarin.Views.DetailView;assembly=TestProjectXamarin"
             x:Class="TestProjectXamarin.Views.Menu.MasterDetail">
    <MasterDetailPage.Master>
        <Local:MasterPage x:Name="masterpage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <DetailView:InfoScreen1/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>